    "git-api",
    "util",
    "tool",
    "utility"
  ],
  "author": {
    "name": "Tam√°s Benke",
    "email": "tombenke@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/tombenke/gitdig/issues"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/tombenke/gitdig/blob/master/LICENSE"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/tombenke/gitdig.git"
  },
  "bin": {
    "gitdig": "./bin/cli.js"
  },
  "main": "index",
  "dependencies": {
    "async": "^2.5.0",
    "base64-coder-node": "^1.0.7",
    "bluebird": "^3.5.0",
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "compare-versions": "^3.0.1",
    "github": "^9.2.0",
    "js-yaml": "^3.9.1",
    "latest-version": "^3.1.0",
    "lodash": "^4.17.4",
    "ossindexjs": "^2.0.0",
    "readline-sync": "^1.4.7"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "coveralls": "^2.11.15",
    "cross-env": "^5.0.5",
    "eslint": "^4.4.1",
    "gulp": "^3.8.10",
    "gulp-debug": "^3.1.0",
    "gulp-markdown": "^1.0.0",
    "gulp-mustache": "^3.0.0",
    "gulp-rename": "^1.2.0",
    "gulp-through": "^0.4.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.2.0",
    "pre-push": "^0.1.1",
    "seneca-stub": "^1.2.2",
    "vinyl-buffer": "^1.0.0"
  },
  "scripts": {
    "lint": "eslint lib/** bin/** config/**.js",
    "test": "cross-env NODE_ENV=test mocha --recursive 'lib/**/*.spec.js'",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- --timeout 15000 --recursive 'lib/**/*.spec.js'",
    "coveralls": "NODE_ENV=test istanbul cover _mocha --report lcovonly -- --timeout 15000 --recursive 'lib/**/*.spec.js' -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "docgen": "gulp docgen"
  },
  "pre-push": [
    "lint",
    "test"
  ]
}
